<Window x:Class="CoLlegEAppLe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoLlegEAppLe"
        mc:Ignorable="d"
        Title="" Height="450" Width="800">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="0 0 0 40"/>
        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="Auto" Height="Auto">
            <Button Click="Btn_tabl1_Click" Margin="10" Width="Auto" Height="Auto" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Content="Оценки"/>
            <Button Click="Btn_tabl2_Click" Margin="10" Width="Auto" Height="Auto" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Content="Студенты"/>
            <Button Click="Btn_tabl3_Click" Margin="10" Width="Auto" Height="Auto" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Content="Специальности"/>
            <Button Click="Btn_tabl4_Click" Margin="10" Width="Auto" Height="Auto" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Content="Предметы"/>
        </DockPanel>
    </Grid>
</Window>
